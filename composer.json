{
    "type": "library",
    "name": "yousign/zdd-message-bundle",
    "description": "Assert Zero Downtime Deployment compliance for messages",
    "license": "MIT",
    "keywords": ["dev", "zdd"],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.1.0",
        "symfony/console": "^6.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.1",
        "symfony/browser-kit": "^6.2",
        "symfony/framework-bundle": "^6.2",
        "symfony/yaml": "^6.2",
        "symfony/messenger": "^6.2",
        "thecodingmachine/safe": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "Yousign\\ZddMessageBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yousign\\ZddMessageBundle\\Tests\\": "tests/"
        }
    }
}
